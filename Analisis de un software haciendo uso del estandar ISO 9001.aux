\@writefile{toc}{\contentsline {subsection}{\numberline {2.1}Objetivo y campo de acci\'on}{3}\protected@file@percent }
\@writefile{toc}{\contentsline {paragraph}{Este programa esta desarrollado para cubrir las principales necesidades de aquellas empresas que se dedican a la venta minorista de productos con c\'odigo de barras o c\'odigos propios}{3}\protected@file@percent }
\@writefile{toc}{\contentsline {paragraph}{este software fue desarrollado por SolverMedia SL. organizaci\'on que la estableci\'o e implement\'o la aplicaci\'on en el a\~no 2008 y aun mantiene su sistema de gesti\'on de la calidad y mejorando continuamente su eficacia.}{3}\protected@file@percent }
\@writefile{toc}{\contentsline {subsection}{\numberline {2.2}Sistema de gesti\'on de calidad}{3}\protected@file@percent }
\@writefile{toc}{\contentsline {paragraph}{El producto se desarroll\'o con el fin de satisfacer las necesidades b\'asicas de las PYMES la cual es mantener un control digital y seguro que se encarga de mantener una imagen fiel de la situaci\'on econ\'omica de la empresa, ademas de contabilizar el inventario existente en dicha entidad.}{3}\protected@file@percent }
\@writefile{toc}{\contentsline {paragraph}{Se desarroll\'o esta aplicaci\'on para que tuviera compatibilidad con el sistema operativo Windows en cualquier version vigente. Cuenta con una version libre con varias funcionalidades, sin embargo se puede comprar una licencia para apoyar a los desarrolladores.}{3}\protected@file@percent }
\@writefile{toc}{\contentsline {paragraph}{El software cuenta con un manual de usuario donde detalla los siguientes puntos:}{3}\protected@file@percent }
\@writefile{toc}{\contentsline {paragraph}{Especifica los requisitos m\IeC {\'\i }nimos que debe que tener el hardware y software del equipo en el que se va a instalar el programa para que funcione me manera optima}{3}\protected@file@percent }
\@writefile{toc}{\contentsline {paragraph}{Muestra la usuario el conjunto de funcionalidades principales con el que cuenta el sistema las cuales dan soluciones a las siguientes necesidades}{4}\protected@file@percent }
\@writefile{toc}{\contentsline {paragraph}{Esta secci\'on le permite al usuario detallar los datos de su empresa, establecer el dise\~no de la factura, Realizar copias de seguridad autom\'atica para evitar perdida de datos, detalles de impuestos y los precios}{5}\protected@file@percent }
\@writefile{toc}{\contentsline {paragraph}{Con esta secci\'on el usuario podr\'a registrarse los productos, m\'etodos de pagos, empleados,c\'odigos de productos y los proveedores}{6}\protected@file@percent }
\@writefile{toc}{\contentsline {paragraph}{Permite al encargado de la caja el poder seleccionar los productos ingresados con su respectivo c\'odigo, precio y la cantidad para as\IeC {\'\i } poder generar una factura que ser\'a entregada al cliente}{7}\protected@file@percent }
